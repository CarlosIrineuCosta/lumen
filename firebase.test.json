{
  "hosting": {
    "public": "frontend",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "storage": {
    "rules": "storage.rules"
  },
  "emulators": {
    "auth": {
      "port": 9290
    },
    "storage": {
      "port": 9291
    },
    "hub": {
      "port": 9499
    },
    "logging": {
      "port": 9599
    },
    "ui": {
      "enabled": true,
      "port": 9699
    },
    "singleProjectMode": true
  }
}
